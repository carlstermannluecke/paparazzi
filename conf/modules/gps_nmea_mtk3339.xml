<!DOCTYPE module SYSTEM "module.dtd">

<module name="gps_nmea_mtk3339" dir="gps">
  <doc>
    <description>
      Addon driver for NMEA mode of MTK3339 enabling higher update rate
    </description>
    <configure name="MTK_BAUD" value="B38400" description="UART baud rate"/>
    <configure name="MTK_RATE" value="B5" description="Update Rate in Hz"/>
  </doc>
  <makefile target="ap">
    <configure name="MTK_BAUD" value="B38400" description="UART baud rate"/>
    <configure name="MTK_RATE" value="B5" description="Update Rate in Hz"/>

    <file name="gps_nmea_mtk3339.c" dir="modules/gps"/>

    <define name="MTK_BAUD" value="$(MTK_BAUD)"/>
    <define name="MTK_RATE" value="$(MTK_RATE)"/>
  </makefile>
</module>
