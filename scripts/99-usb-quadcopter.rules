# install via cp 99-usb-quadcopter.rules /etc/udev/rules.d
# get serial number and id via usb-devices command
# file needs to be owned by root
# execute /etc/init.d/udev restart after copying file
SUBSYSTEM=="tty", ATTRS{idVendor}=="1d50", ATTRS{idProduct}=="6018", ATTRS{serial}=="BDE5A90B", SYMLINK+="magicprobe"
SUBSYSTEM=="tty", ATTRS{idVendor}=="1d50", ATTRS{idProduct}=="603d", ATTRS{serial}=="35323930044732333C003700", SYMLINK+="uav"
SUBSYSTEM=="tty", ATTRS{idVendor}=="1cf1", ATTRS{idProduct}=="001d", ATTRS{serial}=="A8XIXR5M", SYMLINK+="uav"
